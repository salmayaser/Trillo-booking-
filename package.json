{
  "name": "Trillo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:scss": "sass sass/styles.scss css/styles.css -w",
    "compile:scss": "sass sass/styles.scss css/styles.comp.css ",
    "prefix": "postcss -use autoprefixer -b 'last 10 versions' css/styles.comp.css -o css/styles.prefix.css",
    "compress:css": "postcss --use cssnano <css/styles.prefix.css> css/styles.css",
    "build": "npm-run-all compile:scss prefix compress:css"
  },
  "author": "salma",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "concat": "^1.0.3",
    "css-minify": "^2.0.0",
    "cssnano": "^7.0.3",
    "postcss-cli": "^11.0.0",
    "sass": "^1.76.0"
  }
}
